from turtle import *

def sun():
    color('red', 'yellow')
    begin_fill()
    while True:
        forward(200)
        left(170)
        if abs(pos()) < 1:
            break

def dashedLine():
    for i in range(10):
        forward(10)
        penup()
        forward(10)
        pendown()

if __name__ == "__main__":
    # sun()
    dashedLine()
    end_fill()
    done()
