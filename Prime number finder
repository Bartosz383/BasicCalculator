def sieve(n):
    candidate = [1 for x in range(n+1)]
    prime = []
    for i in range(2,n+1):
        if candidate[i] != 0:
            prime.append(i)
            for j in range(i+i,n+1,i):
                candidate[j]=0
    return prime
f = open("numbers.txt", "x")
f.write(str(sieve(2400))[1:-1])
f.close()
print(*sieve(10), sep=", ")
